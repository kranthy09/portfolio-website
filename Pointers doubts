-------
Address of x is 2000,
#include <stdio.h>
int main()
{
int x[2][3];
for(int i =0;i<2;i++)
{
for(int j =0;j<3;j++)
{
scanf("%d", &x[i] [j]) ;
}
}
printf("%d %d %d", x+3, *(x+3), *(x+2) + 3);
return 0;
}
